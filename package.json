{
  "name": "health-progress-tracker",
  "version": "1.0.0",
  "description": "A web application to track health progress with weight and waist measurements",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc && webpack --mode=production",
    "dev": "concurrently \"tsc --watch\" \"webpack --mode=development --watch\" \"nodemon dist/server.js\"",
    "start": "npm run build && node dist/server.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "express": "^4.18.2",
    "sqlite3": "^5.1.6",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@types/jest": "^29.5.8",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "ts-loader": "^9.5.1",
    "html-webpack-plugin": "^5.6.0",
    "copy-webpack-plugin": "^11.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "supertest": "^6.3.3",
    "@types/supertest": "^6.0.2",
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2"
  },
  "keywords": ["health", "tracker", "progress", "weight", "measurements"],
  "author": "",
  "license": "MIT"
}